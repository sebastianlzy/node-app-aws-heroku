{
  "name": "node-app",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": "12.19.0"
  },
  "dependencies": {
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "antd": "^4.14.0",
    "body-parser": "^1.19.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "lodash": "^4.17.21",
    "nodemon": "^2.0.7",
    "pg": "^8.5.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-scripts": "4.0.3",
    "web-vitals": "^1.0.1"
  },
  "devDependencies": {
    "concurrently": "^4.0.1"
  },
  "proxy": "http://localhost:5000/",
  "scripts": {
    "client:build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "client": "yarn start",
    "server": "nodemon -r dotenv/config server/index.js",
    "server:test": "jest",
    "dev": "concurrently --kill-others \"yarn server\" \"react-scripts start\"",
    "start": "node server/index.js",
    "heroku-postbuild": "npm run start:build"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
